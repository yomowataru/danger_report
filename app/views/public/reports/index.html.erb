<div class="container">
  <div class="row">
    <div class="col-lg-10 text-left">
      <% @reports.each do |reports| %>
        <table class="table table-borderless">
          <tr>
            <td><%= reports.id %></td>
            <td><%= link_to reports.introduction, report_path(reports.id) %></td>
          </tr>
        </table>
      <% end %>
    </div>
  </div>
</div>